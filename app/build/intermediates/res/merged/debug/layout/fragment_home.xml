<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="pls.certination.com.certination.MainActivity"
    android:background="@drawable/certi_home_back1">


    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:id="@+id/ib_pls"
        android:layout_marginLeft="5dp"
        android:background="@android:color/transparent"
        android:src="@drawable/plslogo_mini"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:onClick="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="95dp"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="PERFECT LEARNING"
        android:id="@+id/textView"
        android:textSize="10dp"
        android:layout_below="@+id/ib_pls"
        android:lines="2"
        android:textColor="@android:color/black"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="45dp"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="SYSTEM"
        android:textSize="10dp"
        android:textColor="@android:color/black"
        android:id="@+id/textView2"
        android:layout_alignBottom="@+id/textView"
        android:layout_alignRight="@+id/ib_pls"
        android:layout_alignEnd="@+id/ib_pls" />

    <ImageView
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/imageButton"
        android:src="@drawable/train_mini"
        android:background="@android:color/transparent"
        android:layout_alignTop="@+id/ib_pls"
        android:layout_alignLeft="@+id/textView3"
        android:layout_alignStart="@+id/textView3" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="TRAINING AND"
        android:textSize="10dp"
        android:textColor="@android:color/black"
        android:id="@+id/textView3"
        android:layout_alignTop="@+id/textView"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="DEVELOPMENT"
        android:textSize="10dp"
        android:textColor="@android:color/black"
        android:id="@+id/textView4"
        android:layout_alignTop="@+id/textView2"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:id="@+id/imageButton2"
        android:src="@drawable/icon_careers_mini"
        android:background="@android:color/transparent"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/imageButton5"
        android:layout_alignStart="@+id/imageButton5" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="INTERNSHIPS"
        android:textSize="10dp"
        android:textColor="@android:color/black"

        android:id="@+id/textView5"
        android:layout_below="@+id/imageButton2"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignLeft="@+id/imageButton2"
        android:layout_alignStart="@+id/imageButton2" />

    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:id="@+id/imageButton3"
        android:background="@android:color/transparent"
        android:src="@drawable/job_mini"
        android:layout_below="@+id/textView2"
        android:layout_alignRight="@+id/textView2"
        android:layout_alignEnd="@+id/textView2"
        android:layout_marginTop="53dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="JOBS"
        android:id="@+id/textView6"
        android:textSize="10dp"

        android:paddingTop="3dp"
        android:paddingLeft="5dp"
        android:textColor="@android:color/black"
        android:layout_below="@+id/imageButton3"
        android:layout_alignLeft="@+id/textView2"
        android:layout_alignStart="@+id/textView2" />

    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:id="@+id/imageButton4"
        android:layout_marginLeft="5dp"
        android:src="@drawable/guest_mini"
        android:background="@android:color/transparent"
        android:layout_alignTop="@+id/imageButton3"
        android:layout_alignLeft="@+id/textView3"
        android:layout_alignStart="@+id/textView3" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="GUEST"
        android:textSize="10dp"
        android:paddingLeft="3dp"
        android:textColor="@android:color/black"
        android:id="@+id/textView7"
        android:layout_below="@+id/imageButton4"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="LECTURES"
        android:id="@+id/textView8"
        android:textSize="10dp"
        android:paddingLeft="5dp"
        android:textColor="@android:color/black"
        android:layout_below="@+id/textView7"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:id="@+id/imageButton5"
        android:layout_marginRight="5dp"
        android:src="@drawable/workshop_mini"
        android:background="@android:color/transparent"
        android:layout_alignTop="@+id/imageButton4"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="WORKSHOP"
        android:id="@+id/textView9"
        android:textSize="10sp"
        android:textColor="@android:color/black"
        android:layout_below="@+id/imageButton4"
        android:layout_alignLeft="@+id/imageButton5"
        android:layout_alignStart="@+id/imageButton5" />

    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:id="@+id/imageButton6"
        android:background="@android:color/transparent"
        android:layout_marginTop="48dp"
        android:src="@drawable/career_mini"
        android:layout_below="@+id/textView8"
        android:layout_alignLeft="@+id/imageButton3"
        android:layout_alignStart="@+id/imageButton3" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CAREER"
        android:id="@+id/textView10"
        android:textSize="10dp"
        android:textColor="@android:color/black"
        android:layout_below="@+id/imageButton6"
        android:layout_alignLeft="@+id/textView6"
        android:layout_alignStart="@+id/textView6" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="COUNSELLING"
        android:id="@+id/textView11"
        android:textSize="10dp"
        android:textColor="@android:color/black"
        android:layout_below="@+id/textView10"
        android:layout_alignLeft="@+id/imageButton6"
        android:layout_alignStart="@+id/imageButton6" />

    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:id="@+id/imageButton7"
        android:background="@android:color/transparent"
        android:src="@drawable/think_mini"
        android:layout_alignTop="@+id/imageButton6"
        android:layout_alignLeft="@+id/textView8"
        android:layout_alignStart="@+id/textView8" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="APTITUDE"
        android:id="@+id/textView12"
        android:textSize="10dp"
        android:textColor="@android:color/black"
        android:layout_alignTop="@+id/textView11"
        android:layout_alignRight="@+id/textView8"
        android:layout_alignEnd="@+id/textView8" />

    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:id="@+id/imageButton8"
        android:src="@drawable/resume_mini"
        android:background="@android:color/transparent"
        android:layout_alignTop="@+id/imageButton7"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="RESUME"
        android:textSize="10dp"
        android:paddingLeft="10dp"
        android:textColor="@android:color/black"
        android:id="@+id/textView13"
        android:layout_below="@+id/imageButton8"
        android:layout_alignLeft="@+id/imageButton8"
        android:layout_alignStart="@+id/imageButton8" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="BUILDER"
        android:textSize="10dp"
        android:paddingLeft="10dp"
        android:textColor="@android:color/black"
        android:id="@+id/textView14"
        android:layout_below="@+id/textView13"
        android:layout_alignLeft="@+id/textView13"
        android:layout_alignStart="@+id/textView13" />

</RelativeLayout>
</ScrollView>